def venus(sisi):
    luas = sisi*sisi
    return luas

def alfatah(P,L):
    luas = P * L
    return luas

sisi = int(input("Masukkan Sisi : "))
print("Luas Persegi adalah",venus(sisi))
    
P = int(input("Masukan panjang : "))
L = int(input("Masukan Lebar : "))
print("Luas persegi panjang adalah",alfatah(P,L))
